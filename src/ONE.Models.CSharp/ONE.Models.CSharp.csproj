<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <!--For Release packages update the VersionPrefix and remove any VersionSuffix-->
    <VersionPrefix>4.1.0</VersionPrefix>
    <!--For Feature packages update the VersionSuffix with the Jira ticket number-->
    <VersionSuffix></VersionSuffix>
    <LangVersion>latest</LangVersion>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Company>Aquatic Informatics Inc.</Company>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Authors>OpenSourceAquatics</Authors>
    <PackageTags>Aquatic, Informatics, Aquarius, ONE, models, csharp</PackageTags>
    <PackageId>Aquarius.$(AssemblyName)</PackageId>
    <Product>Aquarius.$(AssemblyName)</Product>
    <Copyright>2022</Copyright>
    <Description>C# data model classes generated from shared protocol buffer messages for use within the ONE ecosystem.</Description>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageRequireLicenseAcceptance>True</PackageRequireLicenseAcceptance>
    <PackageReleaseNotes>
      Version 4.1.0
      - Removed priority/status enums and changed priority/status on Activity model to integers
      Version 4.0.0
      - Removed old models related to Activities
      - Added new Activity models
      Version 3.0.0
      - Removed Id from HistorianData model
      - Added Regulatory, Warning, Goal to EnumLimit
    </PackageReleaseNotes>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Google.Protobuf" Version="3.19.1" />
  </ItemGroup>

</Project>
