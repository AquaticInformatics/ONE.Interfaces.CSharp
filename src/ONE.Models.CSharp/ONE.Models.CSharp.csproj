<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <!--For Release packages update the VersionPrefix and remove any VersionSuffix-->
    <VersionPrefix>4.24.0</VersionPrefix>
    <!--For Feature packages update the VersionSuffix with alpha and an incremental number. E.g. alpha1, alpha2...-->
    <VersionSuffix></VersionSuffix>
    <LangVersion>latest</LangVersion>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Company>Aquatic Informatics Inc.</Company>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Authors>OpenSourceAquatics</Authors>
    <PackageTags>Aquatic, Informatics, Aquarius, ONE, models, csharp</PackageTags>
    <PackageId>Aquarius.$(AssemblyName)</PackageId>
    <Product>Aquarius.$(AssemblyName)</Product>
    <Copyright>2023</Copyright>
    <Description>C# data model classes generated from shared protocol buffer messages for use within the ONE ecosystem.</Description>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageRequireLicenseAcceptance>True</PackageRequireLicenseAcceptance>
    <PackageReleaseNotes>
    Version 4.24.0
    - Added SignalR OneHub models.
    </PackageReleaseNotes>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Google.Protobuf" Version="3.19.1" />
  </ItemGroup>

</Project>
