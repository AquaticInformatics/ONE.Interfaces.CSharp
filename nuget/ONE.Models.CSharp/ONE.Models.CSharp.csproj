<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <!--For Release packages update the VersionPrefix and remove any VersionSuffix-->
    <VersionPrefix>2.5.0</VersionPrefix>
    <!--For Feature packages update the VersionSuffix with the Jira ticket number-->
    <VersionSuffix></VersionSuffix>
    <LangVersion>latest</LangVersion>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Company>Aquatic Informatics Inc.</Company>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Authors>OpenSourceAquatics</Authors>
    <PackageTags>Aquatic, Informatics, Aquarius, ONE, models, csharp</PackageTags>
    <PackageId>Aquarius.$(AssemblyName)</PackageId>
    <Product>Aquarius.$(AssemblyName)</Product>
    <Copyright>2022</Copyright>
    <Description>C# data model classes generated from shared protocol buffer messages for use within the ONE ecosystem.</Description>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageRequireLicenseAcceptance>True</PackageRequireLicenseAcceptance>
    <PackageReleaseNotes>
      Version 2.5.0
      - Added new HistorianData model
      Version 2.4.0
      - Added new fields to Configuration model
      Version 2.3.0
      - Added I18N properties to notification template
      Version 2.2.0
      - Added notificationFlag to limit model
      Version 2.1.1
      - Added Terms and Conditions fields back to user model
      Version 2.1.0
      - Added Schedule and ScheduleType models
      Version 2.0.0
      - Updated to ONE.Models.CSharp namespace
      - Added license
    </PackageReleaseNotes>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Google.Protobuf" Version="3.19.1" />
  </ItemGroup>

</Project>
